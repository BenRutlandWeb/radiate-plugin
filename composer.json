{
  "name": "brw/wp-base-plugin",
  "description": "A WordPress plugin boilerplate",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Rutland",
      "email": "info@benrutlandweb.co.uk"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Plugin\\": "app/",
      "Radiate\\": "Radiate/"
    },
    "files": [
      "functions.php"
    ]
  },
  "require": {
    "php": "^7.3",
    "brw/radiate-framework": "@dev"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../vendor/brw/radiate-framework"
    }
  ],
  "scripts": {
    "post-create-project-cmd": [
      "npm install"
    ]
  }
}
